<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.PolizaHijoEntity" table="poliza_hijo" schema="tp_dds">
        <composite-id mapped="true" class="tpdds.PolizaHijoEntityPK">
            <key-property name="dni">
                <column name="dni" sql-type="varchar(8)" length="8"/>
            </key-property>
            <key-property name="nroPoliza">
                <column name="nro_poliza" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>