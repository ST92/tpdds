<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.DireccionEntity" table="direccion" schema="tp_dds">
        <id name="idDireccion">
            <column name="id_direccion" sql-type="int(11)"/>
        </id>
        <property name="nombreCalle">
            <column name="nombre_calle" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="numCalle">
            <column name="num_calle" sql-type="int(11)"/>
        </property>
        <property name="numDpto">
            <column name="num_dpto" sql-type="varchar(4)" length="4"/>
        </property>
    </class>
</hibernate-mapping>