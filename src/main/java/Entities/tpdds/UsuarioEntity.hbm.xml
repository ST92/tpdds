<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.UsuarioEntity" table="usuario" schema="tp_dds">
        <id name="idUsuario">
            <column name="id_usuario" sql-type="int(11)"/>
        </id>
        <property name="usuario">
            <column name="usuario" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="idEnumTipoUsuario">
            <column name="id_enumTipoUsuario" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>