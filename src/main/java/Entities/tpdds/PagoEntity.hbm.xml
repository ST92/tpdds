<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.PagoEntity" table="pago" schema="tp_dds">
        <id name="idPago">
            <column name="id_pago" sql-type="int(11)"/>
        </id>
        <property name="mes">
            <column name="mes" sql-type="int(11)"/>
        </property>
        <property name="monto">
            <column name="monto" sql-type="float" precision="-1"/>
        </property>
        <property name="hora">
            <column name="hora" sql-type="date"/>
        </property>
        <property name="fecha">
            <column name="fecha" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>