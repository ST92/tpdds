<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.ModeloEntity" table="modelo" schema="tp_dds">
        <id name="idModelo">
            <column name="id_modelo" sql-type="int(11)"/>
        </id>
        <property name="idMarca">
            <column name="id_marca" sql-type="int(11)"/>
        </property>
        <property name="nombre">
            <column name="nombre" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="roboPorModelo">
            <column name="robo_por_modelo" sql-type="float" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>