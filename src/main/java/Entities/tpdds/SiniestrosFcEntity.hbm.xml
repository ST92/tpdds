<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.SiniestrosFcEntity" table="siniestros_fc" schema="tp_dds">
        <id name="idSiniestros">
            <column name="id_siniestros" sql-type="int(11)"/>
        </id>
        <property name="descripcion">
            <column name="descripcion" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="valor">
            <column name="valor" sql-type="float" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>