<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.PaisEntity" table="pais" schema="tp_dds">
        <id name="idPais">
            <column name="id_pais" sql-type="int(11)"/>
        </id>
        <property name="nombrePais">
            <column name="nombre_pais" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>