<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.HijoEntity" table="hijo" schema="tp_dds">
        <id name="dni">
            <column name="dni" sql-type="int(11)"/>
        </id>
        <property name="fechaNac">
            <column name="fecha_nac" sql-type="date"/>
        </property>
        <property name="idEnumSexo">
            <column name="id_enumSexo" sql-type="int(11)"/>
        </property>
        <property name="idEnumEstadoCivil">
            <column name="id_enumEstadoCivil" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>