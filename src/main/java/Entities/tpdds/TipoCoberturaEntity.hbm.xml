<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.TipoCoberturaEntity" table="tipo_cobertura" schema="tp_dds">
        <id name="idCobertura">
            <column name="id_cobertura" sql-type="int(11)"/>
        </id>
        <property name="idEnumTipoCobertura">
            <column name="id_enumTipoCobertura" sql-type="int(11)"/>
        </property>
        <property name="descripcion">
            <column name="descripcion" sql-type="varchar(150)" length="150"/>
        </property>
    </class>
</hibernate-mapping>