<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.LocalidadEntity" table="localidad" schema="tp_dds">
        <id name="idLocalidad">
            <column name="id_localidad" sql-type="int(11)"/>
        </id>
        <property name="nombreLocalidad">
            <column name="nombre_localidad" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="codigoPostal">
            <column name="codigo_postal" sql-type="varchar(6)" length="6"/>
        </property>
        <property name="idProvincia">
            <column name="id_provincia" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>