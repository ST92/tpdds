<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tpdds.ProvinciaEntity" table="provincia" schema="tp_dds">
        <id name="idProvincia">
            <column name="id_provincia" sql-type="int(11)"/>
        </id>
        <property name="nombreProvincia">
            <column name="nombre_provincia" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>